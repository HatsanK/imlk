{
    "manifest_version": 2,

    "name": "imlk",
    "description": "그리마스 메뉴 번역",
    "version": "1.0",

		"content_scripts": [
			{
				"matches": ["http://imas.gree-apps.net/*"],
				"css": ["custom.css"]
				}
			],


    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

		"converted_from_user_script": true,

    "permissions": [
			"activeTab",
			"http://imas.gree-apps.net/"
    ],

		"web_accessible_resources": [
      "images/*"
    ]
}
